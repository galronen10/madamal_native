{
  "extends": "expo/tsconfig.base",
  "include": ["src", "App.tsx"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@data/*": ["src/data/*"],
      "@screens/*": ["src/screens"],
      "@models/*": ["src/models/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
